        <Button Content="Ver" HorizontalAlignment="Left" Margin="161,13,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="Button_Click_1"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="http://www.google.es" VerticalAlignment="Top" Width="226" Name="url"/>
        <Label Content="Pagina web" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Image HorizontalAlignment="Left" Height="60" Margin="10,95,0,0" VerticalAlignment="Top" Width="85" Name="icono" Source="{Binding imagen}" Stretch="None"/>
        <Button Content="Guardar" HorizontalAlignment="Left" Height="30" Margin="133,125,0,0" VerticalAlignment="Top" Width="103" Click="Button_Click_2"/>
    </Grid>
</Window>
