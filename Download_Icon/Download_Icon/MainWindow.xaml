<Window x:Class="Download_Icon.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Download_Icon" Height="173.881" Width="342.313" ResizeMode="NoResize">
    <Grid>
        <Button Content="Ver" HorizontalAlignment="Left" Margin="208,53,0,0" VerticalAlignment="Top" Width="103" Height="31" Click="Button_Click_1"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="50,14,0,0" TextWrapping="Wrap" Text="http://www.google.es" VerticalAlignment="Top" Width="261" Name="url"/>
        <Label Content="Url : " HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Image HorizontalAlignment="Left" Height="60" Margin="50,53,0,0" VerticalAlignment="Top" Width="85" Name="icono" Source="{Binding imagen}" Stretch="None"/>
        <Button Content="Guardar" HorizontalAlignment="Left" Height="31" Margin="208,95,0,0" VerticalAlignment="Top" Width="103" Click="Button_Click_2"/>
    </Grid>
</Window>
